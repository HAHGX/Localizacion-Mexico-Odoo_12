<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record model="ir.actions.server" id="connection_tool_server_action">
        <field name="name">Server Action Import Data</field>
        <field name="model_id" eval="ref('connection_tool.model_connection_tool_configure')"/>
        <field name="state">code</field>
        <field name="sequence">5</field>
        <field name="code">
record._cron_connection_tool()
        </field>
    </record>


    <record id="connection_tool_ir_cron" model="ir.cron">
        <field name="name">CRON: Import DAT</field>
        <field name="model_id" ref="connection_tool.model_connection_tool_import_wiz"/>
        <field name="state">code</field>
        <field name="code">model.import_calculation()</field>
        <field name="interval_number">90</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

</odoo>